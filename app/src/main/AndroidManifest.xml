<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ikiler.travel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:name=".util.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <meta-data
            android:name="android.max_aspect"
            android:value="1.778" />
        <meta-data
            android:name="com.baidu.lbsapi.API_KEY"
            android:value="P3G0czAdwF4MCG5dhLM0yBEOgNkqBPUc" />

        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.example.cameraalbumtest.fileprovier"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/filepaths" />
        </provider>

        <activity android:name=".MainActivity" />
        <activity android:name=".MeishiActivity" />
        <activity android:name=".BijiActivity" />
        <activity
            android:name=".ui.PhoneActivity"
            android:label="电话本" />
        <activity android:name=".TianQiActivity" />
        <activity android:name=".DiTuActivity" />
        <activity android:name=".JianDianActivity" />
        <activity android:name=".MeishiDemo" />
        <activity android:name=".ui.NoteEditActivity" />

        <service
            android:name="com.baidu.location.f"
            android:enabled="true"
            android:process=":remote" />

        <activity android:name=".ui.LoginActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity android:name=".ui.Register" />
        <activity
            android:name=".ui.MainContent"
            android:label="@string/app_name"
            android:parentActivityName=".Base.BaseActivity"
            android:theme="@style/AppTheme">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.ikiler.travel.Base.BaseActivity" />
        </activity>
        <activity android:name=".ui.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.Food.FoodListActivity"
            android:label="美食管理" />
        <activity
            android:name=".ui.Food.FoodEditActivity"
            android:label="美食编辑" />
        <activity
            android:name=".ui.Spot.SpotActivity"
            android:label="景点管理" />
        <activity android:name=".ui.Spot.SpotEditActivity" />
        <activity android:name=".ui.TicketShowActivity" />
        <activity
            android:name=".ui.MyTicketActivity"
            android:label="@string/title_activity_my_ticket"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ui.NoteActivity"
            android:label="添加电话本"
            android:theme="@style/AppTheme" />
        <activity android:name=".ui.AddPhoneActivity"></activity>
    </application>

</manifest>